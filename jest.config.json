{
  "collectCoverageFrom": [
    "**/packages/*/src/**/*.{js,jsx,ts,tsx}",
    "!**/node_modules/**",
    "!**/__tests__/**",
    "!**/__mocks__/**",
    "!**/*.d.ts"
  ],
  "testPathIgnorePatterns": [
    "/node_modules/",
    "/dist/"
  ],
  "transform": {
    "^.+\\.[jt]sx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "/node_modules/"
  ]
}
