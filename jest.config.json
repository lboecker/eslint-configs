{
  "collectCoverageFrom": [
    "**/packages/*/src/**/*.{js,jsx,ts,tsx}",
    "!**/__mocks__/**",
    "!**/__tests__/**",
    "!**/*.d.ts",
    "!**/node_modules/**"
  ],
  "testPathIgnorePatterns": ["/node_modules/", "/dist/"],
  "transform": {
    "^.+\\.[jt]sx?$": "babel-jest"
  },
  "transformIgnorePatterns": ["/node_modules/"]
}
